[
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  {
    "label": "Python: Litestar app",
    "adapter": "Debugpy",
    "request": "launch",
    "module": "litestar",
    "justMyCode": false,
    "cwd": "$ZED_WORKTREE_ROOT",
    "args": [
      "--app",
      "app.main:app",
      "run",
      "--reload",
      "--debug",
      "--port=8010"
    ]
  },
  {
    "label": "Python: Litestar app [uvicorn]",
    "adapter": "Debugpy",
    "request": "launch",
    "module": "uvicorn",
    "justMyCode": false,
    "args": ["app.main:app", "--reload", "--log-level=debug", "--port=8010"]
  }
]
